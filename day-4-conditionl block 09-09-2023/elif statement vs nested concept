number=input("enter a number")
number=int(number)



if(number<50):
  if number==10:
   print("number is equal t0 10")
  elif number==20:
   print("number is equal t0 20")
  elif number==30:
    print("number is equal t0 30")
  else:
    print("number not equal")
else:
  print("given number is more than 50")